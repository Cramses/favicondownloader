{
  "manifest_version": 3,
  "name": "Favicon Downloader",
  "version": "1.0.0",
  "description": "Download the favicon of the current website",
  "permissions": [
    "activeTab",
    "downloads",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
